<!DOCTYPE HTML>
<html>
<head>
  <title>Profile main thread</title>

  <style type="text/css">
    body, html {
      font-family: sans-serif;
    }

    .vis-item.popart {
      background-color: red;
    }
  </style>

  <script src="vis.js/dist/vis.js"></script>
  <link href="vis.js/dist/vis.css" rel="stylesheet" type="text/css" />
</head>
<body>

<p>
  A basic timeline. You can move and zoom the timeline, and select items.
</p>

<div id="visualization"></div>

<script type="text/javascript">
  // DOM element where the Timeline will be attached
  var container = document.getElementById('visualization');

  // Make groups.
  var groups = new vis.DataSet();
  for (var g = 0; g < [== $max_depth ==]; ++g) {
    groups.add({id: g, content: g});
  }

  // Create a DataSet (allows two way data-binding)
  var items = new vis.DataSet([== $profile_data ==]);

  // Configuration for the Timeline
  var options = {};

  // Create a Timeline
  var timeline = new vis.Timeline(container);
  timeline.setOptions({groupOrder: 'content', stack: false});
  timeline.setGroups(groups);
  timeline.setItems(items);
</script>
</body>
</html>
